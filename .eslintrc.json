{
    "extends": [
      "zavatta"
    ],
    "rules": {
      "no-console": "off",
      "callback-return": "off",
      "max-statements-per-line": "off",
      "no-catch-shadow": "off",
      "indent": [
        "error",
        2
      ],
      "max-len": [
        "warn",
        {
          "code": 120
        }
      ],
      "no-sync": 0,
      "camelcase": 0
    }
  }
